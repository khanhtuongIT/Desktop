<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="HostConfiguration">
      <section name="ProductInfo" type="System.Configuration.NameValueSectionHandler, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089, Custom=null" />
    </sectionGroup>
  </configSections>

  <HostConfiguration>

    <Plugins>
      <add key="PluginsPath" value=""/>
      <add key="PluginConfigFile" value="Resources/Plugins.config"/>
    </Plugins>

    <Theme>
      <add key="ActiveSkinName" value="Office 2007 Blue"/>
      <add key="ProductTitle" value="ThanhThuy"/>
      <add key="ProductIcon" value="Resources\Icons\fastcashier.ico"/>
      <add key="SplashBitmap" value="Resources\Bitmaps\ecm-plash-screen.png"/>
      <add key="LogonBackgroundBitmap" value="Resources/Bitmaps/logonbg.png"/>
    </Theme>

    <CultureInfo>
      <add key="DefaultCultureInfo" value="vi-VN"/>
      <add key="DefaultCultureInfoPath" value="Resources\Default"/>
      <add key="CultureInfoPath" value="Resources\Lang"/>
      <add key="CurrentCultureInfo" value=""/>
    </CultureInfo>

    <DisplayFormat>
      <add key="DateTimeFormat" value="dd/MM/yyyy"/>
      <add key="LongDateTimeFormat" value="dd/MM/yyyy hh:mm:ss tt"/>
      <add key="NumericFormat" value="###,###,###,###,###,###"/>
    </DisplayFormat>

    <CurrentUser>
      <add key="UserName" value=""/>
    </CurrentUser>

    <Location>
      <add key="Id_Cuahang_Ban" value="1" />
    </Location>

  </HostConfiguration>

  <StartupObjects>
    <FrmMainUseRibbon>
      <add key="Assembly" value="GoobizFrame.Windows.dll"/>
      <add key="Type" value="GoobizFrame.Windows.Forms.FrmMainUseRibbon"/>
      <add key="Caption" value="Main Form"/>
      <add key="RunAfterLogon" value="False"/>
      <add key="Exec" value="True"/>
    </FrmMainUseRibbon>

    <Frmware_Hdbanhang_noVAT_Hhban>
      <add key="Assembly" value="Ecm.Ware.dll"/>
      <add key="Type" value="Ecm.Ware.Forms.Frmware_Hdbanhang_noVAT_Hhban"/>
      <add key="Caption" value="Bán hàng"/>
      <add key="RunAfterLogon" value="False"/>
      <add key="Exec" value="False"/>
    </Frmware_Hdbanhang_noVAT_Hhban>

  </StartupObjects>

  <ReportOptions>
    <rptHdbanhang_noVAT>
      <add key="Type" value="Ecm.Ware.Reports.rptHdbanhang_noVAT"/>
      <add key="Caption" value="In hóa đơn bán lẻ"/>
      <add key="Description" value="Sử dụng trong các chức năng: x,y,z,..."/>
      <add key="PrintPreview" value="True"/>
