    </Reference>
   
    <Webservices>
      <add key="BarService"             value="http://localhost:51000/BarService.asmx"/>
      <add key="MasterService"          value="http://localhost:51000/MasterService.asmx"/>
      <add key="RexService"             value="http://localhost:51000/RexService.asmx"/>
